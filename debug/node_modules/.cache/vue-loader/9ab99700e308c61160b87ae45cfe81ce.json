   <div class=\"switch-first\">{{ $t(\"DeviceStatus_NNSF_Label\")}}</div>\r\n              <div class=\"switch-state\">\r\n                 {{ data1 }}%\r\n              </div>\r\n              <div class=\"text-align-center\">\r\n                <el-slider v-model=\"data1\" show-input  ></el-slider>\r\n              </div>\r\n            </div>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row :gutter=\"30\" class=\"mt-27\">\r\n          <el-col :span=\"6\">\r\n            <div class=\"switch-content bg-off\">\r\n              <div class=\"switch-first\">{{ $t(\"DeviceStatus_SWSF_Label\")}}</div>\r\n              <div class=\"switch-state\">\r\n                {{ data2 }}%\r\n              </div>\r\n              <div class=\"text-align-center\">\r\n                <el-slider v-model=\"data2\" show-input @change=\"moveSWSF\" ></el-slider>\r\n              </div>\r\n            </div>\r\n          </el-col>\r\n          <el-col :span=\"6\">\r\n            <div class=\"switch-content bg-on\">\r\n              <div class=\"switch-first\">{{ $t(\"DeviceStatus_SESF_Label\")}}</div>\r\n              <div class=\"switch-state\">{{ data3 }}%</div>\r\n              <div class=\"text-align-center\">\r\n                <el-slider v-model=\"data3\" show-input @change=\"moveSESF\"  ></el-slider>\r\n              </div>\r\n            </div>\r\n          </el-col>\r\n          <el-col :span=\"6\">\r\n            <div class=\"switch-content bg-off\">\r\n              <div class=\"switch-first\">{{ $t(\"DeviceStatus_NSWD_Label\")}}</div>\r\n              <div class=\"switch-state\">{{ data4 }}%</div>\r\n              <div class=\"text-align-center\">\r\n                <el-slider v-model=\"data4\" show-input></el-slider>\r\n              </div>\r\n            </div>\r\n          </el-col>\r\n          <el-col :span=\"6\">\r\n            <div class=\"switch-content bg-on\">\r\n              <div class=\"switch-first\">{{ $t(\"DeviceStatus_SSWD_Label\")}}</div>\r\n              <div class=\"switch-state\">{{ data5 }}%</div>\r\n              <div class=\"text-align-center\">\r\n                <el-slider v-model=\"data5\" show-input></el-slider>\r\n                <a class=\"switch-retry\">{{ $t(\"DeviceStatus_Retry_Action\")}}</a>\r\n              </div>\r\n            </div>\r\n          </el-col>\r\n        </el-row>\r\n         <el-row :gutter=\"30\" class=\"mt-27\">\r\n          <el-col :span=\"6\">\r\n            <div class=\"switch-content bg-off\">\r\n              <div class=\"switch-first\">サンバイザー</div>\r\n              <div class=\"switch-state\">{{ data6 }}%</div>\r\n              <div class=\"text-align-center\">\r\n                <el-slider v-model=\"data6\" show-input></el-slider>\r\n              </div>\r\n            </div>\r\n          </el-col>\r\n        </el-row>\r\n        <!-- モダール ——> ヒント -->\r\n        <el-dialog :visible.sync=\"openDialogVisible\" class=\"openDialog\">\r\n          <div class=\"message-content\">{{$t(\"DeviceStatus_Fan_Label\")}}{{ $t(\"DeviceStatus_On_Message\")}}</div>\r\n          <div class=\"text-align-right\">\r\n            <el-button class=\"cancelText\" @click=\"openDialogVisible = false\">{{ $t(\"DeviceStatus_Cancel_Action\")}}</el-button>\r\n            <el-button class=\"successBtn\" @click=\"tunOnOffLight = false\">{{ $t(\"DeviceStatus_Confirm_Action\")}}</el-button>\r\n          </div>\r\n        </el-dialog>\r\n         <el-dialog :visible.sync=\"openDialogVisible1\" class=\"openDialog\">\r\n          <div class=\"message-content\">{{$t(\"DeviceStatus_Light_Label\")}}{{ $t(\"DeviceStatus_On_Message\")}}</div>\r\n          <div class=\"text-align-right\">\r\n            <el-button class=\"cancelText\" @click=\"openDialogVisible1 = false\">{{ $t(\"DeviceStatus_Cancel_Action\")}}</el-button>\r\n            <el-button class=\"successBtn\" @click=\"tunOnOffFan = false\">{{ $t(\"DeviceStatus_Confirm_Action\")}}</el-button>\r\n          </div>\r\n        </el-dialog>\r\n         <el-dialog :visible.sync=\"openDialogVisible2\" class=\"openDialog\">\r\n          <div class=\"message-content\">{{$t(\"DeviceStatus_Pump_Label\")}}{{ $t(\"DeviceStatus_On_Message\")}}</div>\r\n          <div class=\"text-align-right\">\r\n            <el-button class=\"cancelText\" @click=\"openDialogVisible2 = false\">{{ $t(\"DeviceStatus_Cancel_Action\")}}</el-button>\r\n            <el-button class=\"successBtn\" @click=\"tunOnOffPump = false\">{{ $t(\"DeviceStatus_Confirm_Action\")}}</el-button>\r\n          </div>\r\n        </el-dialog>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nexport default {\r\n  name: 'deviceStatus',\r\n  data() {\r\n    return {\r\n      fanisOn: true,\r\n      fanisOff: false,\r\n      fanisDanger: false,\r\n      \r\n      lightisOn: true,\r\n      lightisOff: false,\r\n      lightisDanger: false,\r\n      \r\n      pumpisOn: true,\r\n      pumpisOff: false,\r\n      pumpisDanger: false,\r\n      \r\n      nnsfisOn: true,\r\n      nnsfisOff: false,\r\n      nnsfisDanger: false,\r\n      \r\n      sesfisOn: true,\r\n      sesfisOff: false,\r\n      sesfisDanger: false,\r\n      \r\n      swsfisOn: true,\r\n      swsfisOff: false,\r\n      swsfisDanger: false,\r\n      \r\n      nswnisOn: true,\r\n      nswnisOff: false,\r\n      nswnisDanger: false,\r\n      \r\n      sswnisOn: true,\r\n      sswnisOff: false,\r\n      sswnisDanger: false,\r\n      \r\n      \r\n      fan_enable: '啟用中',\r\n      fan_action: this.$t('DeviceStatus_Off_Action'),\r\n      \r\n      light_enable: '啟用中',\r\n      light_action: this.$t('DeviceStatus_Off_Action'),\r\n      \r\n      pump_enable: '啟用中',\r\n      pump_action: this.$t('DeviceStatus_On_Action'),\r\n      \r\n      data1: 5,\r\n      data2: 42,\r\n      data3: 0,\r\n      data4: 42,\r\n      data5: 0,\r\n      data6: 0,\r\n      \r\n      openDialogVisible: false,\r\n      openDialogVisible1: false,\r\n      openDialogVisible2: false,\r\n    }\r\n  },\r\n  created() {\r\n    setInterval( this.getSensorsData, 6000 )\r\n  },\r\n  methods: {\r\n     moveNNSF() {\r\n        alert('Move NNSF' + this.data1)\r\n        this.$ajax({\r\n           method: 'GET',\r\n           url: 'http://47.74.5.223:7542/do?gwid=KASO03HM&ofid=FAN&ofvalue=1'\r\n          })\r\n     },\r\n     \r\n     moveSWSF() {\r\n        console.log('SWSF')\r\n        alert('Move NNSF' + this.data2)\r\n        this.$ajax({\r\n           method: 'GET',\r\n           url: 'http://47.74.5.223:7542/do?gwid=KASO03HM&ofid=FAN&ofvalue=1'\r\n          })\r\n     },\r\n     \r\n     moveSESF() {\r\n        alert('Move NNSF' + this.data3)\r\n     },\r\n     \r\n     moveNSWN() {\r\n        alert('Move NNSF' + this.data4)\r\n     },\r\n     \r\n     moveSSWN() {\r\n        alert('Move NNSF' + this.data5)\r\n     },\r\n     \r\n     moveXXXX() {\r\n        alert('Move XXX' + this.data6)\r\n     },\r\n     \r\n     tunOnOffFan() {\r\n     \r\n       // alert('tunOnOffFan')\r\n        if( this.fanisOff ) {\r\n         this.$ajax({\r\n           method: 'GET',\r\n           url: 'http://47.74.5.223:7542/do?gwid=KASO03HM&ofid=FAN&ofvalue=1'\r\n          })\r\n       }\r\n       \r\n       if( this.fanisOn ) {\r\n         this.$ajax({\r\n           method: 'GET',\r\n           url: 'http://47.74.5.223:7542/do?gwid=KASO03HM&ofid=FAN&ofvalue=2'\r\n          })\r\n       }\r\n       \r\n        this.openDialogVisible1 = false\r\n     \r\n     },\r\n     \r\n     \r\n     \r\n      tunOnOffLight() {\r\n     \r\n       //alert('tunOnOffLight')\r\n       \r\n       if( this.lightisOff ) {\r\n         this.$ajax({\r\n           method: 'GET',\r\n           url: 'http://47.74.5.223:7542/do?gwid=KASO03HM&ofid=LIGHT&ofvalue=1'\r\n          })\r\n       }\r\n       \r\n       if( this.lightisOn ) {\r\n         this.$ajax({\r\n           method: 'GET',\r\n           url: 'http://47.74.5.223:7542/do?gwid=KASO03HM&ofid=LIGHT&ofvalue=2'\r\n          })\r\n       }\r\n       \r\n        this.openDialogVisible = false\r\n     },\r\n     \r\n      tunOnOffPump() {\r\n     \r\n       // alert('tunOnOffPump')\r\n       \r\n      if( this.pumpisOff ) {\r\n         this.$ajax({\r\n           method: 'GET',\r\n           url: 'http://47.74.5.223:7542/do?gwid=KASO03HM&ofid=WATER&ofvalue=1'\r\n          })\r\n       }\r\n       \r\n       if( this.pumpisOn ) {\r\n         this.$ajax({\r\n           method: 'GET',\r\n           url: 'http://47.74.5.223:7542/do?gwid=KASO03HM&ofid=WATER&ofvalue=2'\r\n          })\r\n       }\r\n       \r\n        this.openDialogVisible2 = false\r\n     },\r\n     \r\n      getSensorsData() {\r\n       this.$ajax({\r\n         method: 'GET',\r\n         url: 'http://47.74.5.223:7542/query?gwid=KASO03HM'\r\n       })\r\n       .then( res => {\r\n          var objstr = JSON.stringify(res.data)\r\n          console.log(objstr)\r\n          var objres = JSON.parse(objstr)\r\n          \r\n          this.data1 = objres.NNSF\r\n          this.data2 = objres.SESF\r\n          this.data3 = objres.SWSF\r\n          this.data4 = objres.NSWN\r\n          this.data5 = objres.SSWN\r\n          \r\n          if( this.data1 > 0 ) {\r\n          \r\n             this.nnsfisOn = true\r\n             this.nnsfisOff = false\r\n             this.nnsfisDanger = false\r\n             \r\n          } else {\r\n          \r\n             this.nnsfisOn = false\r\n             this.nnsfisOff = true\r\n             this.nnsfisDanger = false\r\n          \r\n          }\r\n          \r\n           if( this.data2 > 0 ) {\r\n          \r\n             this.sesfisOn = true\r\n             this.sesfisOff = false\r\n             this.sesfisDanger = false\r\n             \r\n          } else {\r\n          \r\n             this.sesfisOn = false\r\n             this.sesfisOff = true\r\n             this.sesfisDanger = false\r\n          \r\n          }\r\n          \r\n           if( this.data3 > 0 ) {\r\n          \r\n             this.swsfisOn = true\r\n             this.sw