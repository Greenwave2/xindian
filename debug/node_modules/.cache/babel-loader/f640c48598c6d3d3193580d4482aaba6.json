\r\n        padding: 0 30px;\r\n        .message-content {\r\n          padding: 0 11px 15px;\r\n          font-family: PingFangSC-Regular;\r\n          font-size: 16px;\r\n          color: #333333;\r\n          line-height: 26px;\r\n        }\r\n        .el-button {\r\n          &.successBtn,\r\n          &.cancelBtn {\r\n            margin-left: 15px;\r\n            padding: 0;\r\n            width: 76px;\r\n            height: 36px;\r\n            line-height: 36px;\r\n            border-radius: 2px;\r\n            color: #fff;\r\n            font-family: PingFangSC-Regular;\r\n            font-size: 14px;\r\n            border: 1px solid #00cc99;\r\n            background: #00cc99;\r\n          }\r\n\r\n          &.cancelText {\r\n            padding: 12px 0;\r\n            font-family: PingFangSC-Regular;\r\n            font-size: 14px;\r\n            color: rgba(0, 0, 0, 0.65);\r\n            text-align: left;\r\n            border: 1px solid #fff;\r\n            background: #fff;\r\n          }\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n/*\r\n*歷史數據画面:HistoricalData-> inputカレンダーの様式\r\n*/\r\n.history-message {\r\n  .el-date-editor.el-input {\r\n    width: 177px;\r\n    .el-input__inner {\r\n      height: 36px;\r\n      line-height: 20px;\r\n      border-radius: 2px;\r\n      color: #333;\r\n      border: 1px solid #aaaaaa;\r\n      padding-left: 9px;\r\n    }\r\n    .el-input__prefix {\r\n      right: 5px;\r\n      left: auto;\r\n      .el-input__icon {\r\n        line-height: 0;\r\n      }\r\n    }\r\n    .el-input__suffix {\r\n      .el-input__suffix-inner {\r\n        display: none;\r\n      }\r\n    }\r\n  }\r\n}\r\n.el-picker-panel__body-wrapper {\r\n  // el-date-picker__time-header\r\n  .el-date-picker__time-header {\r\n    .el-date-picker__editor-wrap {\r\n      .el-input {\r\n        .el-input__inner {\r\n          background: #ffffff;\r\n          border: 1px solid #dddddd;\r\n          border-radius: 2px;\r\n        }\r\n      }\r\n    }\r\n 